{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "dotnet: debug build",
			"type": "process",
			"command": "dotnet",
			"args": [
				"build",
				"${workspaceFolder}/Coroner.sln",
				"-c Release",
				"/property:GenerateFullPaths=true",
				"/consoleloggerparameters:NoSummary",
				"--no-dependencies"
			],
			"problemMatcher": [
				"$msCompile"
			]
		}
	]
}